{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ],
        "functions": {
          "dist/*.js": {
            "maxDuration": 60
          },
          "src/*.ts": {
            "maxDuration": 60
          }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}